{
  "name": "@peerio/peerio-icebear",
  "version": "2.11.1",
  "description": "Peerio Icebear client library",
  "main": "src/index.js",
  "scripts": {
    "prepush": "npm run validate",
    "postcheckout": "opt --out npm-update --exec \"npm install && npm update\"",
    "postmerge": "opt --out npm-update --exec \"npm install && npm update\"",
    "precommit": "opt --out lint --exec \"npm run lint\"",
    "postcommit": "git fetch --prune origin '+refs/tags/*:refs/tags/*'",
    "commitmsg": "validate-commit-msg",
    "mocha": "mocha test/**/*.test.js --require test/global-setup --compilers js:babel-register -R progress --timeout 15000",
    "start": "npm run mocha -- -w",
    "test": "nyc --reporter=text --reporter=html npm run mocha",
    "validate": "npm-run-all --serial lint",
    "lint": "eslint . --fix --ext .js,.jsx",
    "release": "./scripts/release.sh",
    "doc:serve": "documentation serve src -c ./docs/src/config.yml -a public -a protected -a private -a undefined -g -w -t ./docs/src/theme",
    "doc:lint": "documentation lint src",
    "doc:build": "documentation build src -a public -f md -o docs/md/API.md && documentation build src -c ./docs/src/config.yml -a public -a protected -a private -a undefined -g -t ./docs/src/theme -f html -o ./docs/html"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeerioTechnologies/peerio-icebear.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PeerioTechnologies/peerio-icebear/issues"
  },
  "homepage": "https://github.com/PeerioTechnologies/peerio-icebear#readme",
  "peerDependencies": {
    "blake2s-js": "1.2.2",
    "bluebird": "3.5.0",
    "buffer": "5.0.8",
    "eventemitter3": "2.0.3",
    "lodash": "4.17.4",
    "mobx": "3.3.0",
    "moment": "2.18.1",
    "peerio-translator": "git+https://github.com/PeerioTechnologies/peerio-translator.git#icebear",
    "scrypt-async": "1.3.1",
    "socket.io-client": "2.0.3",
    "tweetnacl": "1.0.0"
  },
  "dependencies": {
    "blake2s-js": "1.2.2",
    "bluebird": "3.5.0",
    "buffer": "5.0.8",
    "eventemitter3": "2.0.3",
    "lodash": "4.17.4",
    "mobx": "3.3.0",
    "moment": "2.18.1",
    "peerio-translator": "git+https://github.com/PeerioTechnologies/peerio-translator.git#icebear",
    "scrypt-async": "1.3.1",
    "socket.io-client": "2.0.3",
    "tweetnacl": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-console-kungfu": "git+https://github.com/PeerioTechnologies/babel-plugin-console-kungfu.git",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-register": "6.26.0",
    "babel-types": "6.26.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "cz-conventional-changelog": "2.0.0",
    "documentation": "5.3.2",
    "eslint-config-peerio": "1.19.11",
    "husky": "0.14.3",
    "mocha": "3.5.3",
    "mock-require": "2.0.2",
    "npm-run-all": "4.1.1",
    "nyc": "11.2.1",
    "opt-cli": "1.5.2",
    "rimraf": "2.6.2",
    "sinon": "4.0.0",
    "sinon-chai": "2.14.0",
    "source-map-support": "0.4.18",
    "standard-version": "4.2.0",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 120,
      "subjectPattern": ".+",
      "helpMessage": "Convention: https://goo.gl/3TgTsh"
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "node ./scripts/postbump.js"
    }
  },
  "devEngines": {
    "node": "7.x",
    "npm": "4.x"
  }
}
